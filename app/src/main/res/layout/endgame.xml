<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:background="#e6c902"
    android:layout_height="match_parent"
    tools:context="com.salinesingularity.nhwltrs.salinesingularity2018scoutingapp.MatchData">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <CheckBox
                android:id="@+id/endgameVaultBoostCheckBox"
                android:layout_width="361dp"
                android:layout_height="38dp"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:text="Boost"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/endgameVaultForceCheckBox" />

            <CheckBox
                android:id="@+id/endgameVaultForceCheckBox"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:text="Force"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/endgameVaultTextView" />

            <View
                android:id="@+id/view6"
                android:layout_width="0dp"
                android:layout_height="5dp"
                android:layout_gravity="center"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:background="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/teleopAllianceSwitchTextView" />

            <View
                android:id="@+id/view7"
                android:layout_width="0dp"
                android:layout_height="5dp"
                android:layout_gravity="center"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:background="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/endgameScaleTextView" />

            <View
                android:id="@+id/view8"
                android:layout_width="0dp"
                android:layout_height="5dp"
                android:layout_gravity="center"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:background="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/endgameOpponentSwitchTextView" />

            <Button
                android:id="@+id/endgameScaleAddButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="16dp"
                android:text="+"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/endgameScaleMinusButton" />

            <TextView
                android:id="@+id/endgameScaleCounterTextView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:text="0"
                android:textAlignment="center"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/endgameScaleMinusButton"
                app:layout_constraintEnd_toStartOf="@+id/endgameScaleAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgameScaleMinusButton" />

            <Button
                android:id="@+id/endgameScaleMinusButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="46dp"
                android:text="-"
                app:layout_constraintEnd_toStartOf="@+id/endgameScaleAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgameScaleTextView" />

            <TextView
                android:id="@+id/endgameScaleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Scale"
                android:textColor="#000000"
                android:textSize="25sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view6" />

            <View
                android:id="@+id/view10"
                android:layout_width="0dp"
                android:layout_height="5dp"
                android:layout_gravity="center"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="16dp"
                android:background="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/endgameVaultBoostCheckBox" />

            <Button
                android:id="@+id/endgameAllianceSwitchAddButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="16dp"
                android:text="+"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/endgameAllianceSwitchMinusButton" />

            <Button
                android:id="@+id/endgameAllianceSwitchMinusButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="46dp"
                android:text="-"
                app:layout_constraintEnd_toStartOf="@+id/endgameAllianceSwitchAddButton"
                app:layout_constraintTop_toTopOf="@+id/teleopAllianceSwitchTextView" />

            <TextView
                android:id="@+id/endgameAllianceSwitchCounterTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:text="0"
                android:textAlignment="center"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/endgameAllianceSwitchMinusButton"
                app:layout_constraintEnd_toStartOf="@+id/endgameAllianceSwitchAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgameAllianceSwitchMinusButton" />

            <TextView
                android:id="@+id/teleopAllianceSwitchTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Alliance Switch"
                android:textColor="#000000"
                android:textSize="25sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/view9"
                android:layout_width="0dp"
                android:layout_height="5dp"
                android:layout_gravity="center"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:background="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/endgamePortalTextView" />

            <TextView
                android:id="@+id/endgameOpponentSwitchTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Opponents Switch"
                android:textColor="#000000"
                android:textSize="25sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view7" />

            <Button
                android:id="@+id/endgameOpponentsSwitchMinusButton2"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="46dp"
                android:text="-"
                app:layout_constraintEnd_toStartOf="@+id/endgameOpponentsSwitchAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgameOpponentSwitchTextView" />

            <TextView
                android:id="@+id/endgameOpponentsSwitchCounterTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:text="0"
                android:textAlignment="center"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/endgameOpponentsSwitchMinusButton2"
                app:layout_constraintEnd_toStartOf="@+id/endgameOpponentsSwitchAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgameOpponentsSwitchMinusButton2" />

            <Button
                android:id="@+id/endgameOpponentsSwitchAddButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="16dp"
                android:text="+"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/endgameOpponentsSwitchMinusButton2" />

            <TextView
                android:id="@+id/endgamePortalTextView"
                android:layout_width="wrap_content"
                android:layout_height="41dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Portal"
                android:textColor="#000000"
                android:textSize="25sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view8" />

            <Button
                android:id="@+id/endgamePortalMinusButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="46dp"
                android:text="-"
                app:layout_constraintEnd_toStartOf="@+id/endgamePortalAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgamePortalTextView" />

            <TextView
                android:id="@+id/endgamePortalCounterTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:text="0"
                android:textAlignment="center"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/endgamePortalMinusButton"
                app:layout_constraintEnd_toStartOf="@+id/endgamePortalAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgamePortalMinusButton" />

            <Button
                android:id="@+id/endgamePortalAddButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="16dp"
                android:text="+"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/endgamePortalMinusButton" />

            <TextView
                android:id="@+id/endgameVaultTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Vault"
                android:textColor="#000000"
                android:textSize="25sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view9" />

            <TextView
                android:id="@+id/climbTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Climb"
                android:textColor="#000000"
                android:textSize="25sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view10" />

            <Button
                android:id="@+id/endgameVaultMinusButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="46dp"
                android:text="-"
                app:layout_constraintEnd_toStartOf="@+id/endgameVaultAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgameVaultTextView" />

            <TextView
                android:id="@+id/endgameVaultCounterTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:text="0"
                android:textAlignment="center"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/endgameVaultMinusButton"
                app:layout_constraintEnd_toStartOf="@+id/endgameVaultAddButton"
                app:layout_constraintTop_toTopOf="@+id/endgameVaultMinusButton" />

            <Button
                android:id="@+id/endgameVaultAddButton"
                android:layout_width="41dp"
                android:layout_height="38dp"
                android:layout_marginEnd="16dp"
                android:text="+"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/endgameVaultMinusButton" />

            <Button
                android:id="@+id/climbStartStopButton"
                android:layout_width="170dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="2dp"
                android:text="Start"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view10" />

            <TextView
                android:id="@+id/climbTimerTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:fontFamily="monospace"
                android:text="00.000"
                android:textSize="25sp"
                app:layout_constraintBottom_toBottomOf="@+id/climbStartStopButton"
                app:layout_constraintEnd_toStartOf="@+id/climbStartStopButton"
                app:layout_constraintTop_toTopOf="@+id/climbStartStopButton" />

            <Switch
                android:id="@+id/parkSwitch"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:text="Park"
                android:textSize="25sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/climbStartStopButton" />

            <Button
                android:id="@+id/endMatchButton"
                android:layout_width="200dp"
                android:layout_height="100dp"
                android:layout_marginTop="8dp"
                android:text="End Match"
                android:textSize="25dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/parkSwitch" />
        </android.support.constraint.ConstraintLayout>
    </ScrollView>

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

</android.support.constraint.ConstraintLayout>